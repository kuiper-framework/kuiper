{
    "name": "kuiper/tars",
    "type": "library",
    "description": "Tars integration for PHP ",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "kuiper/rpc": "^0.8",
        "guzzlehttp/psr7": "^2.0",
        "symfony/validator": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\tars\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\tars\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\tars\\config\\TarsClientConfiguration"
            ]
        }
    }
}
