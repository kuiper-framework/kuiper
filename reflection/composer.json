{
    "name": "kuiper/reflection",
    "type": "library",
    "description": "Php reflection utility",
    "keywords": [
        "reflection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@163.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "phpstan/phpdoc-parser": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\reflection\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\reflection\\ReflectionConfiguration"
            ]
        }
    }
}
