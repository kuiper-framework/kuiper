{
    "name": "kuiper/tracing-client",
    "description": "Bindings for PHP OpenTracing API",
    "keywords": [
        "opentracing",
        "trace",
        "tracing"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "kuiper/http-client": "^0.8",
        "opentracing/opentracing": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\tracing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\tracing\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\tracing\\TracingConfiguration"
            ]
        }
    }
}
