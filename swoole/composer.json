{
    "name": "kuiper/swoole",
    "type": "library",
    "description": "Swoole server util",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-swoole": "*",
        "webmozart/assert": "^1.0",
        "laminas/laminas-code": "^3.4",
        "kuiper/di": "^0.8",
        "kuiper/event": "^0.8",
        "kuiper/logger": "^0.8",
        "kuiper/serializer": "^0.8",
        "psr/http-server-handler": "^1.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\swoole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\swoole\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\swoole\\config\\FoundationConfiguration",
                "kuiper\\swoole\\config\\GuzzleHttpMessageFactoryConfiguration",
                "kuiper\\swoole\\config\\DiactorosHttpMessageFactoryConfiguration"
            ]
        }
    }
}
