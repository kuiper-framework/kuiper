{
    "name": "kuiper/jsonrpc",
    "type": "library",
    "description": "JSONRPC client library for PHP ",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laminas/laminas-code": "^3.4",
        "kuiper/rpc": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\jsonrpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\jsonrpc\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\jsonrpc\\config\\JsonRpcClientConfiguration"
            ]
        }
    }
}
