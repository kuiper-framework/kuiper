{
    "name": "kuiper/db",
    "license": "MIT",
    "description": "A PDO extension that provides lazy connections, auto reconnecting, and fluent query builder",
    "keywords": [
        "mysql",
        "pdo"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "yewenbin@winwin-inc.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "aura/sqlquery": "^2.0",
        "webmozart/assert": "^1.0",
        "kuiper/event": "^0.8",
        "kuiper/helper": "^0.8",
        "kuiper/logger": "^0.8",
        "kuiper/reflection": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\db\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\db\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\db\\DbConfiguration"
            ]
        }
    }
}
