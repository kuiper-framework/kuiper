{
    "name": "kuiper/rpc-registry",
    "type": "library",
    "description": "RPC registry using consul, nacos, etc",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "kuiper/rpc": "^0.8",
        "kuiper/http-client": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\rpc\\registry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\rpc\\registry\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\rpc\\registry\\RpcRegistryConfiguration"
            ]
        }
    }
}
